////////////////////////////
// Add new lines

replace-regex "},\n  {'"
with "},\n\n  {'"

////////////////////////////
// Final adjustments

replace "  ""
with "  '"

replace-regex ""\n"
with "'\n"

replace-regex "",\n"
with "',\n"

replace "",\n"
with "',\n"

replace "": ""
with "': '"

replace "": "
with "': "

replace "\\\\"
with "\"

replace "\\"
with "\"

replace " \""
with " ""

replace "","
with "',"

replace "\',"
with "","

replace "\""
with """

replace " '{{ "
with " !unsafe '{{ " 

replace ": ' #{{ "
with ": !unsafe ' #{{ "

replace ": '#{{ "
with ": !unsafe '#{{ "

replace ": '"{{ "
with ": !unsafe '"{{ "

replace-regex "}\n\n"
with ""

replace-regex "}\n\n"
with "} # type: ignore\n"

replace-regex "    ]\n\n"
with "] # type: ignore\n"

replace-regex ""\n"
with "'\n"
